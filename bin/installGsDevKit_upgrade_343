#
iferr 1 stk
iferr 2 stack
iferr 3 exit 1
display oops
limit bytes 200
set u SystemUser p swordfish
login

input $GS_HOME/shared/repos/GsDevKit_upgradeDevKitImage/bin/installGsDevKit_upgrade

run
	
	UserGlobals
		at: #GsDevKit_Image_Upgrade
		put: 
			(GsuGsDevKit_3_5_0_Upgrade
				upgradeUserName: 'DataCurator'
				upgradeSymbolDictName: #UserGlobals
				sourceGemStoneRelease: (GsuAbstractGemStoneRelease major: 3 minor: 4 patch: 3)).
"
				bootstrapExistingConfigurationList: #( ConfigurationOfGLASS1 );
				yourself).
"
	System myUserProfile removeDictionaryAt: (System myUserProfile symbolList size).
	true
%
commit

errorCount

exit 

